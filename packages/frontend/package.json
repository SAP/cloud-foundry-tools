{
  "name": "cf-signin-ui",
  "displayName": "Cloud Foundry Signin",
  "version": "1.7.9",
  "private": true,
  "description": "Frontend for the cf sign-in",
  "license": "Apache 2.0",
  "publisher": "SAP",
  "scripts": {
    "build": "vue-cli-service build",
    "build-dev": "vue-cli-service build --mode development",
    "ci": "npm-run-all clean build coverage coverage:copy",
    "clean": "rimraf ./dist ./coverage",
    "coverage": "vue-cli-service test:unit",
    "coverage:copy": "shx mkdir -p ../../coverage && shx cp -u ./coverage/lcov.info ../../coverage/lcov_frontend.info",
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service test:unit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "root": true
  },
  "jest": {
    "coverageReporters": [
      "json"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "tempDirectory": "./coverage"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "@mdi/js": "^6.7.96",
    "@sap-devx/webview-rpc": "0.3.1",
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "core-js": "3.6.5",
    "v-tooltip": "^2.1.3",
    "vue": "2.6.12",
    "vue-loading-overlay": "3.3.0",
    "vue-mdijs": "^0.4.3",
    "vuetify": "^2.6.6"
  },
  "devDependencies": {
    "@babel/preset-env": "7.11.5",
    "@vscode/codicons": "^0.0.30",
    "@vue/cli-plugin-babel": "4.5.4",
    "@vue/cli-plugin-unit-jest": "4.5.4",
    "@vue/cli-service": "4.5.4",
    "@vue/test-utils": "1.0.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "husky": "4.2.5",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "mock-socket": "^9.0.2",
    "sass": "1.26.10",
    "sass-loader": "10.0.2",
    "vue-jest": "3.0.6",
    "vue-template-compiler": "2.6.12"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
